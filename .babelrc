{
    "plugins": [
        "react-hot-loader/babel",
        [
            "transform-runtime",
            {
                "helpers": false,
                "polyfill": true,
                "regenerator": true,
                "moduleName": "babel-runtime"
            }
        ],
        [
            "import",
            {
                "libraryName": "antd",
                "libraryDirectory": "es",
                "style": "css"
            }
        ]
    ],
    "presets": [
        [
            "env",
            {
                "modules": false,
                "debug": false,
                "targets": {
                    "browsers": [
                        "> 1%",
                        "last 2 versions"
                    ]
                }
            }
        ],
        "stage-0",
        "react"
    ]
}